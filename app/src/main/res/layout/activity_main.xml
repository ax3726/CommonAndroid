<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <com.lm.base.widget.MyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/lly_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:context="com.lm.base.u_content">

            <Button
                android:id="@+id/btn1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="共享元素动画" />

            <Button
                android:id="@+id/btn2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Materical效果" />
            <Button
                android:id="@+id/btn3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="获取数据" />
            <Button
                android:onClick="installChajian"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="加载插件" />
            <Button
                android:onClick="startChajian"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="启动插件" />
            <Button
                android:onClick="startChajian2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="启动插件2" />
            <Button
                android:onClick="testDownLoad"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="测试下载" />
        </LinearLayout>
    </com.lm.base.widget.MyScrollView>
</layout>